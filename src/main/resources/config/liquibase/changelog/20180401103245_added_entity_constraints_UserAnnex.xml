<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity UserAnnex.
    -->
    <changeSet id="20180401103245-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="status_id"
                                 baseTableName="user_annex"
                                 constraintName="fk_user_annex_status_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_status"/>

        <addForeignKeyConstraint baseColumnNames="user_annexes_id"
                                 baseTableName="user_annex_user_type"
                                 constraintName="fk_user_annex_user_type_user_annexes_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_annex"/>
        <addForeignKeyConstraint baseColumnNames="user_types_id"
                                 baseTableName="user_annex_user_type"
                                 constraintName="fk_user_annex_user_type_user_types_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_type"/>
        
        <addForeignKeyConstraint baseColumnNames="invitee_id"
                                 baseTableName="user_annex"
                                 constraintName="fk_user_annex_invitee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_annex"/>

        <addForeignKeyConstraint baseColumnNames="user_status_history_id"
                                 baseTableName="user_annex"
                                 constraintName="fk_user_annex_user_status_history_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_status_history"/>

    </changeSet>
</databaseChangeLog>
